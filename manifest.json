{
    "manifest_version": 3,
    "name": "URL Hider",
    "version": "1.0",
    "action": {
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "scripting",
        "tabs",
        "webNavigation",
        "storage"
    ],
    "host_permissions": [
        "<all_urls>"
    ]
}